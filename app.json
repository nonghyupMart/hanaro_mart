{
  "expo": {
    "name": "HanaroPlus",
    "slug": "hanaro-plus",
    "version": "1.0.1",
    "orientation": "portrait",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1.0.1",
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.hanaro.hanaroPlus",
      "supportsTablet": false,
      "googleServicesFile": "./GoogleService-info.plist",
      "splash": {
        "backgroundColor": "#ffffff",
        "resizeMode": "cover",
        "image": "./assets/splash/ios/img1242x2436.png",
        "hideExponentText": true
      }
    },
    "android": {
      "versionCode": 2,
      "splash": {
        "mdpi": "./assets/splash/and/img320x480.png",
        "hdpi": "./assets/splash/and/img480x800.png",
        "xhdpi": "./assets/splash/and/img720x1280.png",
        "xxhdpi": "./assets/splash/and/img960x1600.png",
        "xxxhdpi": "./assets/splash/and/img1080x1920.png",
        "hideExponentText": true,
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.hanaroPlus.hanaroPlus",
      "useNextNotificationsApi": true,
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "READ_INTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
